package p2;

public class AllSmoothie {
	
	//스무디 판매수량
	int smoothiecnt;
	
	//스무디 보너스 누적
	double point;
	
	//스무디 가격
	int price;
	int SmoothiePrice;
	int YogurtPrice;
	int PlumPrice;
	int PeachPrice;
		
		public AllSmoothie(int smoothiecnt) {
			this.price = price;
			this.point = (price * 0.1);
	
		}

}

class Yogurt extends AllSmoothie{

	public Yogurt(int smoothiecnt) {
		super(4500);
		
		

	}
	
	@Override
	public String toString() {
		return "플레인 요거트 스무디";
	}

}

class Plum extends AllSmoothie{

	public Plum(int smoothiecnt) {
		super(smoothiecnt);

	}
	
	@Override
	public String toString() {
		return "자두 스무디";
	}

}

class Peach extends AllSmoothie{

	public Peach(int smoothiecnt) {
		super(smoothiecnt);


	}
	
	@Override
	public String toString() {
		return "복숭아 스무디";
	}

}
